    "react-addons-update": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "react-select": "1.0.0-rc.2",
    "superagent": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.14",
    "enzyme": "^2.0.0",
    "expect": "^1.14.0",
    "expect-jsx": "^2.6.0",
    "flow-bin": "^0.35.0",
    "localStorage": "^1.0.3",
    "react-addons-test-utils": "15.3.2",
    "react-scripts": "0.7.0",
